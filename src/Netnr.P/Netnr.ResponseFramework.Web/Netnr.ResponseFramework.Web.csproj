<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<IncludeNativeLibrariesForSelfExtract>true</IncludeNativeLibrariesForSelfExtract>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DocumentationFile>Netnr.ResponseFramework.Web.xml</DocumentationFile>
		<NoWarn>1701;1702;1591</NoWarn>
		<DefineConstants>TRACE;ImportBase;ImportWeb;Ready;App;DataKit;DKController;IpArea;UserAgent;Drawing</DefineConstants>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<DefineConstants>TRACE;ImportBase;ImportWeb;Ready;App;DataKit;DKController;IpArea;UserAgent;Drawing</DefineConstants>
	</PropertyGroup>

	<ItemGroup>
	  <Compile Remove="wwwroot\upload\tmp\**" />
	  <Content Remove="wwwroot\upload\tmp\**" />
	  <EmbeddedResource Remove="wwwroot\upload\tmp\**" />
	  <None Remove="wwwroot\upload\tmp\**" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="6.0.5" />
		<PackageReference Include="Swashbuckle.AspNetCore" Version="6.3.1" />
		<PackageReference Include="Swashbuckle.AspNetCore.Newtonsoft" Version="6.3.1" />
		<PackageReference Include="DeviceDetector.NET" Version="4.3.0" />
		<PackageReference Include="FluentScheduler" Version="5.5.1" />
		<PackageReference Include="IP2Region" Version="1.2.0" />
		<PackageReference Include="SkiaSharp" Version="2.88.1-preview.1" />
		<PackageReference Include="SkiaSharp.NativeAssets.Linux" Version="2.88.1-preview.1" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Netnr.ResponseFramework.Application\Netnr.ResponseFramework.Application.csproj" />
	</ItemGroup>

	<ItemGroup>
		<None Update="db\backup.zip">
		  <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Update="db\netnrf.db">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Update="logs\README.md">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Update="Netnr.ResponseFramework.Web.xml">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
	</ItemGroup>

	<Import Project="..\Netnr.Shared\Netnr.Shared.projitems" Label="Shared" />

</Project>
