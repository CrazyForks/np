<div class="container-fluid">
    <div class="row">
        <div class="col-auto mb-3">
            <div class="btn-group">
                <button class="btn btn-success nrBtnSwaggerUI">SwaggerUI</button>
                <button type="button" class="btn btn-success dropdown-toggle dropdown-toggle-split" data-bs-toggle="dropdown" data-bs-reference="parent">
                    <span class="visually-hidden">Toggle Dropdown</span>
                </button>
                <ul class="dropdown-menu">
                    <li>
                        <button class="dropdown-item nrBtnYamlToJson">
                            YAML
                            <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" fill="currentColor" viewBox="0 0 16 16">
                                <path fill-rule="evenodd" d="M4 8a.5.5 0 0 1 .5-.5h5.793L8.146 5.354a.5.5 0 1 1 .708-.708l3 3a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708-.708L10.293 8.5H4.5A.5.5 0 0 1 4 8z" />
                            </svg>
                            JSON
                        </button>
                    </li>
                    <li>
                        <button class="dropdown-item nrBtnJsonToYaml">
                            JSON
                            <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" fill="currentColor" viewBox="0 0 16 16">
                                <path fill-rule="evenodd" d="M4 8a.5.5 0 0 1 .5-.5h5.793L8.146 5.354a.5.5 0 1 1 .708-.708l3 3a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708-.708L10.293 8.5H4.5A.5.5 0 0 1 4 8z" />
                            </svg>
                            YAML
                        </button>
                    </li>
                    <li><hr class="dropdown-divider"></li>
                    <li>
                        <button class="dropdown-item nrBtnSwaggerToOpenAPI">
                            Swagger
                            <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" fill="currentColor" viewBox="0 0 16 16">
                                <path fill-rule="evenodd" d="M4 8a.5.5 0 0 1 .5-.5h5.793L8.146 5.354a.5.5 0 1 1 .708-.708l3 3a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708-.708L10.293 8.5H4.5A.5.5 0 0 1 4 8z" />
                            </svg>
                            OpenAPI
                        </button>
                    </li>
                </ul>
            </div>
        </div>
        <div class="col-auto mb-3">
            <div class="input-group">
                <button class="btn btn-warning nrTo">转文档</button>
                <select class="form-select nrConfigOnlyJson">
                    <option value="1">仅 JSON</option>
                    <option value="0">不处理</option>
                </select>
            </div>
        </div>
        <div class="col-auto mb-3">
            <div class="btn-group">
                <button type="button" class="btn btn-secondary dropdown-toggle" data-bs-toggle="dropdown">下载</button>
                <ul class="dropdown-menu dropdown-menu-end nrDown">
                    <li><button class="dropdown-item" type="button">Markdown</button></li>
                    <li><button class="dropdown-item" type="button">HTML</button></li>
                    <li><button class="dropdown-item" type="button">Word</button></li>
                    <li><button class="dropdown-item" type="button">PDF</button></li>
                    <li><button class="dropdown-item" type="button">PNG</button></li>
                </ul>
            </div>
        </div>
        <div class="col-auto mb-3">
            <button class="btn btn-dark nr-demo">Demo</button>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <div class="border overflow-hidden position-relative nrEditorSwagger1"></div>
        </div>
        <div class="col-md-6 nrOutBox">
            <div class="d-none border overflow-hidden position-relative nrEditorSwagger2"></div>
            <div class="d-none nrEditorSwagger3">
                <div class="nrMarkdownEditor"></div>
            </div>
            <div class="d-none border overflow-auto position-relative nrSwaggerUI"></div>
        </div>
    </div>
</div>

@Html.Raw(Netnr.SharedApp.QuoteTo.Html("netnrmd.css,netnrmd.js,js-yaml.js,api-spec-converter.js,swagger-ui-dist.css,swagger-ui-dist.js,xml2json.js,monaco-editor"))