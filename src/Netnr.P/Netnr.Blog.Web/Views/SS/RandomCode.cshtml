<div class="@QuoteTo.PanelClass2">
    <div class="row">
        <div class="col-auto mb-2">
            <button class="nrg-btn-build btn btn-warning">生成</button>
        </div>
        <div class="col-auto mb-2">
            <button class="nrg-btn-reset btn btn-secondary">重置</button>
        </div>
        <div class="col-auto">
            <div class="input-group mb-2" style="user-select:none">
                <span class="input-group-text me-3">包含</span>
                <div class="mt-2 form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" id="flag-upper" checked value="A">
                    <label class="form-check-label" for="flag-upper">ABC</label>
                </div>
                <div class="mt-2 form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" id="flag-lower" checked value="a">
                    <label class="form-check-label" for="flag-lower">abc</label>
                </div>
                <div class="mt-2 form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" id="flag-number" checked value="0">
                    <label class="form-check-label" for="flag-number">123</label>
                </div>
                <div class="mt-2 form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" id="flag-symbol" checked value="!">
                    <label class="form-check-label" for="flag-symbol">!@@#</label>
                </div>
            </div>
        </div>
        <div class="col-auto">
            <div class="input-group mb-2">
                <span class="input-group-text">长度</span>
                <input class="nrg-txt-length form-control" id="flag-length" placeholder="长度" type="number" value="16" min="1" max="99" step="1" />
            </div>
        </div>
        <div class="col-auto">
            <div class="input-group mb-2">
                <span class="input-group-text">排除</span>
                <input class="nrg-txt-exclude form-control" id="flag-exclude" value="0oOiI1l" placeholder="排除字符，如：0oOiIlL1" />
            </div>
        </div>
        <div class="col-12">
            <textarea class="nrg-txt-result form-control form-control-lg" rows="9" placeholder="输出"></textarea>
        </div>
    </div>
</div>