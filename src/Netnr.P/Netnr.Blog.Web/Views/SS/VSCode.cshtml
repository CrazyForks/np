<div class="@QuoteTo.PanelClass1">
    <div class="row">
        <div class="col-auto mb-2">
            <select class="nrg-se-language invisible form-select"></select>
        </div>
        <div class="nrg-card-active col-auto mb-2">
            <button class="nrg-btn-run btn btn-warning d-none">Run</button>
            <button class="nrg-btn-preview btn btn-warning d-none">Preview</button>
        </div>
        <div class="col"></div>

        <div class="col-auto mb-2">
            <div class="btn-group">
                <div class="nrg-ddl-list dropdown">
                    <button type="button" class="btn btn-outline-secondary dropdown-toggle" data-bs-toggle="dropdown" data-bs-auto-close="false">
                        列表 List
                    </button>
                    <div class="dropdown-menu p-0 mt-1 border-0">
                        <ol class="nrg-list-file list-group list-group-numbered overflow-auto" style="width:20em" role="button"></ol>
                    </div>
                </div>
                <div class="dropdown mx-1">
                    <button type="button" class="btn btn-outline-secondary dropdown-toggle" data-bs-toggle="dropdown">
                        导出
                    </button>
                    <ul class="dropdown-menu">
                        <li><button class="nrg-btn-export dropdown-item">导出当前</button></li>
                        <li><button class="nrg-btn-export-all dropdown-item">导出全部</button></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="col-auto mb-2">
        </div>
        <div class="col-auto mb-2">
            <div class="btn-group">
                <button class="nrg-btn-new btn btn-outline-secondary">✚ 新建</button>
            </div>
        </div>

        <div class="col-12">
            <div class="nrg-editor"></div>
        </div>
    </div>
</div>