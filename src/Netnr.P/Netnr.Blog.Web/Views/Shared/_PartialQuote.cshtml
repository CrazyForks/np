@model string
@{
    var listQuote = Model.Split(',').ToList();
}
@foreach (var item in listQuote)
{
    switch (item)
    {
        case "MirrorReadOnly":
            if (GlobalTo.GetValue<bool>("ReadOnly"))
            {
                <div class="alert alert-warning text-center p-1 m-0 nrMsiro">Mirror site is read only &nbsp; <a href="https://www.netnr.com">www.netnr.com</a> &nbsp; 镜像站点只读</div>
                <script>
                    setTimeout(function () {
                        var pn = location.pathname.toLowerCase();
                        if (pn.indexOf("/draw/mind") == 0 || pn.indexOf("/draw/code/m") == 0) {
                            document.querySelector(".nrMsiro").style.display = "none"
                        }
                    }, 1000 * 9);
                </script>
            }
            break;
        case "z.css":
            <link href="/lib/jquery-easyui-z/z.css" rel="stylesheet" asp-append-version="true" />
            break;

        case "z.js":
            <script src="/lib/jquery-easyui-z/z.js" asp-append-version="true"></script>
            break;

        case "fun.js":
            <script src="/js/fun.js" asp-append-version="true" async defer></script>
            break;
    }
}