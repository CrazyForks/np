@{
    Layout = null;
    var baseServer = "/file/mxgraph";
}

<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta charset="utf-8" />

    <title>Graph @Html.Raw(AppTo.GetValue("Common:ChineseName"))</title>

    <style>
        /*重写菜单样式*/
        html td.mxPopupMenuItem {
            font-size: 12pt;
            padding: 3px 30px;
        }

        /*全屏*/
        .viewFull .geDiagramContainer {
            position: absolute !important;
            top: 0 !important;
            left: 0 !important;
            right: 0 !important;
            z-index: 1;
        }

        .btns {
            color: #333;
            border-radius: 2px;
            background-color: #f5f5f5;
            border: 1px solid #d8d8d8;
            margin: 0 0 0 8px;
            outline: 0;
            padding: 3px 6px;
            cursor: pointer;
        }
    </style>
</head>
<body>
    @Html.Raw(QuoteTo.Html("loading"))

    <input type="hidden" id="mxBaseServer" value='@baseServer' />

    <link href="@baseServer/grapheditor/styles/grapheditor.css" rel="stylesheet" />

    <script src="/file/draw-graph.js" asp-append-version="true"></script>

    <script type="text/javascript" src="@baseServer/grapheditor/deflate/pako.min.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/deflate/base64.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/jscolor/jscolor.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/sanitizer/sanitizer.min.js"></script>
    <script type="text/javascript" src="@baseServer/mxClient.min.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/js/EditorUi.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/js/Editor.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/js/Sidebar.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/js/Graph.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/js/Format.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/js/Shapes.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/js/Actions.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/js/Menus.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/js/Toolbar.js"></script>
    <script type="text/javascript" src="@baseServer/grapheditor/js/Dialogs.js"></script>
</body>
</html>